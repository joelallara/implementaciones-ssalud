      <DTS:ObjectData>
        <SQLTask:SqlTaskData
          SQLTask:Connection="{C58967E1-D6E6-45BE-BAEB-DD39AA465CE7}"
          SQLTask:SqlStatementSource="&#x9;&#xA;&#x9;DECLARE @Periodo INTEGER&#xA;&#x9;DECLARE @PeriodoCierre INT&#xA;&#x9;SET @Periodo = DW_SALUD.dbo.DevuelvePeriodo(GETDATE())&#xA;&#x9;SET @PeriodoCierre = (SELECT MAX(Id_Cierre_Ejercicio_Anterior) FROM TRANS_CIERRE_EJERCICIO_ANTERIOR)&#xA;&#xA;/* Actualiza TRANS_CIERRE_EJERCICIO_ANTERIOR */&#xA;&#xA;&#x9;-- Lo hace mediante el maximo periodo del ejercicio anterior. Si el periodo a actualizar es el 7 (apertura del ejercicio) debería cambiar de periodo y en los meses siguientes continuar con los demás meses. */&#xA;&#x9;IF @Periodo &lt;&gt; (SELECT MAX(id_periodo) FROM TRANS_CIERRE_EJERCICIO_ANTERIOR)&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;IF RIGHT(@Periodo, 2)= 07&#xA;&#xA;&#x9;&#x9;&#x9;INSERT INTO TRANS_CIERRE_EJERCICIO_ANTERIOR&#xA;&#x9;&#x9;&#x9;SELECT @Periodo, DW_SALUD.dbo.PeriodoAnioSiguiente(CAST(CAST(DW_SALUD.dbo.Devuelve_Fecha(@PeriodoCierre) AS VARCHAR) AS DATETIME))&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;ELSE&#xA;&#xA;&#x9;&#x9;&#x9;INSERT INTO TRANS_CIERRE_EJERCICIO_ANTERIOR&#xA;&#x9;&#x9;&#x9;SELECT @Periodo, @PeriodoCierre&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;&#xA;&#x9;&#x9;&#x9;" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
      </DTS:ObjectData>
    </DTS:Executable>
