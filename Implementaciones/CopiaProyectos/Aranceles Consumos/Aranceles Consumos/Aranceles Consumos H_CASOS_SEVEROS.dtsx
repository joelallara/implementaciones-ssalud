        <SQLTask:SqlTaskData
          SQLTask:Connection="{A7161898-9CA0-4E15-920D-09B3D39D44ED}"
          SQLTask:SqlStatementSource="DECLARE @Periodo INT&#xA;SET @Periodo = (SELECT MAX(dbo.PeriodoSiguiente(CAST(dbo.Devuelve_Fecha(id_periodo)AS VARCHAR(10)))) FROM H_CASOS_SEVEROS)&#xA;&#xA;WHILE @Periodo &lt;= (SELECT MAX(id_periodo) FROM H_ORD_INT_DET)&#xA;BEGIN&#xA;&#x9;DELETE H_CASOS_SEVEROS WHERE id_periodo = @Periodo&#xA;&#xA;&#x9;select&#x9;a11.id_persona  id_persona,&#xA;&#x9;&#x9;sum(a11.imp_prest_int_pag)  imp_prest_int_pag&#xA;&#x9;into #ZZT8GO3QEBZSP000&#xA;&#x9;from&#x9;H_ORD_INT_DET&#x9;a11&#xA;&#x9;&#x9;join&#x9;D_PRODUCTOS&#x9;a12&#xA;&#x9;&#x9;  on &#x9;(a11.id_producto = a12.id_producto)&#xA;&#x9;&#x9;join&#x9;D_PLANES&#x9;a13&#xA;&#x9;&#x9;  on &#x9;(a11.id_plan_producto = a13.id_plan_producto and &#xA;&#x9;&#x9;a11.id_producto = a13.id_producto)&#xA;&#x9;where&#x9;(a12.Id_Clase_Producto in ('S ')&#xA;&#x9; and a13.Id_SubRubro_Ley in (1, 2, 3)&#xA;&#x9; and a11.id_marca_valorizacion in ('P')&#xA;&#x9; and a12.id_segmento not in (3)&#xA;&#x9; and not ((a11.id_producto = 16 and a11.id_plan_producto = 'CC   ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CM   ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CS1  ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CS100')&#xA;&#x9;   or  (a11.id_producto=4 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=20 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=14 and a11.id_plan_producto='Z    ')&#xA;&#x9;   or  (a11.id_producto=20 and a11.id_plan_producto='Z    ')&#xA;&#x9;   or  (a11.id_producto=1 and a11.id_plan_producto='C    ')&#xA;&#x9;   or  (a11.id_producto=4 and a11.id_plan_producto='Z    '))&#xA;&#x9; and a11.id_periodo_prestacion IN (SELECT TOP 12 id_periodo FROM D_PERIODO WHERE id_periodo &lt;= @Periodo ORDER BY id_periodo DESC))&#xA;&#x9;group by&#x9;a11.id_persona &#xA;&#xA;&#x9;select&#x9;a11.id_persona  id_persona,&#xA;&#x9;&#x9;sum(a11.imp_pret_amb_pag)  imp_pret_amb_pag&#xA;&#x9;into #ZZTHE6TQ127SP001&#xA;&#x9;from&#x9;H_ORD_AMB_DET&#x9;a11&#xA;&#x9;&#x9;join&#x9;D_PRODUCTOS&#x9;a12&#xA;&#x9;&#x9;  on &#x9;(a11.id_producto = a12.id_producto)&#xA;&#x9;&#x9;join&#x9;D_PLANES&#x9;a13&#xA;&#x9;&#x9;  on &#x9;(a11.id_plan_producto = a13.id_plan_producto and &#xA;&#x9;&#x9;a11.id_producto = a13.id_producto)&#xA;&#x9;where&#x9;(a12.Id_Clase_Producto in ('S ')&#xA;&#x9; and a13.Id_SubRubro_Ley in (1, 2, 3)&#xA;&#x9; and a11.id_marca_valorizacion in ('P')&#xA;&#x9; and a12.id_segmento not in (3)&#xA;&#x9; and not ((a11.id_producto = 16 and a11.id_plan_producto = 'CC   ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CM   ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CS1  ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='CS100')&#xA;&#x9;   or  (a11.id_producto=4 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=16 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=20 and a11.id_plan_producto='M    ')&#xA;&#x9;   or  (a11.id_producto=14 and a11.id_plan_producto='Z    ')&#xA;&#x9;   or  (a11.id_producto=20 and a11.id_plan_producto='Z    ')&#xA;&#x9;   or  (a11.id_producto=1 and a11.id_plan_producto='C    ')&#xA;&#x9;   or  (a11.id_producto=4 and a11.id_plan_producto='Z    '))&#xA;&#x9; and a11.id_periodo_prestacion IN (SELECT TOP 12 id_periodo FROM D_PERIODO WHERE id_periodo &lt;= @Periodo ORDER BY id_periodo DESC))&#xA;&#x9;group by&#x9;a11.id_persona &#xA;&#xA;&#x9;INSERT INTO H_CASOS_SEVEROS&#xA;&#x9;select&#x9;distinct&#x9;@Periodo,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;coalesce(pa11.id_persona, pa12.id_persona),&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;0,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;0,&#xA;&#x9;&#x9;&#x9;&#x9;&#x9;&#x9;0&#xA;&#x9;from&#x9;#ZZT8GO3QEBZSP000&#x9;pa11&#xA;&#x9;&#x9;full outer join&#x9;#ZZTHE6TQ127SP001&#x9;pa12&#xA;&#x9;&#x9;  on &#x9;(pa11.id_persona = pa12.id_persona)&#xA;&#x9;&#x9;  &#xA;&#x9;UPDATE H_CASOS_SEVEROS&#xA;&#x9;SET importe_int = sumaInt&#xA;&#x9;FROM H_CASOS_SEVEROS A &#xA;&#x9;INNER JOIN (SELECT id_persona, sum(imp_prest_int_pag) AS sumaInt&#xA;&#x9;&#x9;&#x9;&#x9;FROM #ZZT8GO3QEBZSP000&#xA;&#x9;&#x9;&#x9;&#x9;GROUP BY id_persona) AS I&#xA;&#x9;ON (A.id_persona=I.id_persona)&#xA;&#x9;where id_periodo = @Periodo&#xA;&#xA;&#xA;&#x9;UPDATE H_CASOS_SEVEROS&#xA;&#x9;SET importe_amb = sumaAmb&#xA;&#x9;FROM H_CASOS_SEVEROS C &#xA;&#x9;INNER JOIN (SELECT id_persona, sum(imp_pret_amb_pag) AS sumaAmb&#xA;&#x9;&#x9;&#x9;&#x9;FROM #ZZTHE6TQ127SP001&#xA;&#x9;&#x9;&#x9;&#x9;GROUP BY id_persona) AS A&#xA;&#x9;ON (C.id_persona=A.id_persona)&#xA;&#x9;where id_periodo = @Periodo&#xA;&#xA;&#xA;&#x9;UPDATE H_CASOS_SEVEROS&#xA;&#x9;SET total = ROUND(importe_amb+importe_int, 2)&#xA;&#x9;where id_periodo = @Periodo&#xA;&#xA;&#x9;DELETE H_CASOS_SEVEROS&#xA;&#x9;WHERE total &lt; 500000&#xA;&#x9;and id_periodo = @Periodo&#xA;&#xA;&#x9;drop table #ZZT8GO3QEBZSP000&#xA;&#xA;&#x9;drop table #ZZTHE6TQ127SP001&#xA;&#x9;&#xA;&#x9;SET @Periodo = (SELECT MAX(dbo.PeriodoSiguiente(CAST(dbo.Devuelve_Fecha(id_periodo)AS VARCHAR(10)))) FROM H_CASOS_SEVEROS)&#xA;&#xA;END" xmlns:SQLTask="www.microsoft.com/sqlserver/dts/tasks/sqltask" />
